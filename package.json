{
  "name": "latamap-upgrade",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:all": "vitest run && playwright test",
    "format": "biome format --write",
    "lint": "biome lint --write --unsafe",
    "typecheck": "tsc --noEmit",
    "clean": "biome check --write --unsafe && tsc --noEmit && vitest run && playwright test",
    "post-cta-init": "npx create-db@latest",
    "db:generate": "dotenv -e .env -- prisma generate",
    "db:push": "dotenv -e .env -- prisma db push",
    "db:migrate": "dotenv -e .env -- prisma migrate dev",
    "db:studio": "dotenv -e .env -- prisma studio",
    "db:seed": "dotenv -e .env -- prisma db seed",
    "db:reset": "dotenv -e .env -- prisma migrate reset --force",
    "db:repopulate": "npm run db:reset && npm run db:seed"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "@vercel/speed-insights": "^1.3.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.24.0",
    "lucide-react": "^0.544.0",
    "nitro": "^3.0.1-alpha.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-draggable": "^4.5.0",
    "react-range": "^1.10.0",
    "resend": "^6.9.2",
    "sonner": "^2.0.7",
    "tailwindcss": "^4.1.18",
    "topojson-client": "^3.1.0",
    "use-debounce": "^10.0.6",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@playwright/test": "^1.58.2",
    "@tanstack/devtools-vite": "^0.3.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.2.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/topojson-client": "^3.1.5",
    "@vitejs/plugin-react": "^5.0.4",
    "dotenv-cli": "^10.0.0",
    "jsdom": "^27.0.0",
    "prisma": "^7.2.0",
    "srvx": "^0.11.7",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0"
  }
}
